namespace WPFlatbuffers;

struct Vector3 {
  x: float = 0;
  y: float = 0;
  z: float = 0;
}

table Update {
  actorId: string;
  deleted: bool = false;
  speed: float;
  color: ushort;
  position: Vector3;
  rotation: Vector3;
  scale: Vector3;
  up: Vector3;
}

table Diff {
  version: ushort;
  updates: [Update];
}

root_type Diff;
