namespace WPFlatbuffers;

struct Vector3 {
  x: float = 0;
  y: float = 0;
  z: float = 0;
}

struct Float {
  value: float;
}

struct UShort {
  value: ushort;
}

table Update {
  actorId: string;
  deleted: bool = false;
  speed: Float;
  color: UShort;
  position: Vector3;
  rotation: Vector3;
  scale: Vector3;
  up: Vector3;
}

table Diff {
  version: UShort;
  updates: [Update];
}

root_type Diff;
